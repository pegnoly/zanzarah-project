{
  "productName": "zz-bof",
  "version": "0.1.0",
  "identifier": "com.zzbof.app",
  "build": {
    "beforeDevCommand": "pnpm dev --host",
    "devUrl": "http://localhost:1420",
    "beforeBuildCommand": "pnpm build",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [
      {
        "title": "zz-bof",
        "width": 800,
        "height": 600
      }
    ],
    "security": {
      "csp": {
        "img-src": "'self' asset: http://asset.localhost blob: data:"
      },
      "assetProtocol": {
        "enable": true
      }
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}
